        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Level Generator

// Init game
scr_init_game();

// Set room size
room_width = (CELL_WIDTH div 16) * 500;
room_height = (CELL_HEIGHT div 16) * 500;

// set the grid's width and height
var width = room_width div CELL_WIDTH;
var height = room_height div CELL_HEIGHT;

// Create grid
grid = ds_grid_create(width, height);

// Create pathfinding grid
grid_path = mp_grid_create(0, 0, width, height, CELL_WIDTH, CELL_HEIGHT);

// Fill grid with void spaces
ds_grid_set_region(grid, 0, 0, width - 1, height - 1, VOID);
ds_grid_set_region(grid, 2, 2, width - 3, height - 3, FLOOR);

// Starts random number generator
randomize();

// Sets controller
var cx = width div 2;
var cy = height div 2;

// Create instance of the camera
instance_create(0, 0, obj_camera);

// Add the tiles to the grid
for(var yy = 2; yy &lt; height - 2; yy++)
{
    for(var xx = 2; xx &lt; width - 2; xx++)
    {
        if(grid[# xx, yy] == FLOOR)
        {
            if(grid[# xx + 1, yy] != FLOOR) grid[# xx + 1, yy] = WALL;
            if(grid[# xx - 1, yy] != FLOOR) grid[# xx - 1, yy] = WALL;
            if(grid[# xx, yy + 1] != FLOOR) grid[# xx, yy + 1] = WALL;
            if(grid[# xx, yy - 1] != FLOOR) grid[# xx, yy - 1] = WALL;
        }
    }
}

// Chance placing obstacle
var odds = 6;

// Place Obstacles
for(var yy = 4; yy &lt; height - 4; yy++)
{
    for(var xx = 4; xx &lt; width - 4; xx++)
    {
        if(irandom(odds) == odds)
        {
            if(grid[# xx + 1, yy] != WALL &amp;&amp; grid[# xx + 1, yy - 1] != WALL &amp;&amp; grid[# xx, yy - 1] != WALL &amp;&amp; grid[# xx - 1, yy - 1] != WALL &amp;&amp;
                grid[# xx - 1, yy] != WALL &amp;&amp; grid[# xx - 1, yy + 1] != WALL &amp;&amp; grid[# xx, yy + 1] != WALL &amp;&amp; grid[# xx + 1, yy + 1] != WALL)
            {
                grid[# xx, yy] = OBSTACLE;
            }
        }
    }
}

// Place random walls
for(var xx = 2; xx &lt; width - 3; xx++)
{
    var yy = 0;
    var wall_close = false;
    if(irandom(odds) == odds)
    {
        for(var xx_temp = xx; xx_temp &gt; xx - 8 &amp;&amp; !wall_close; xx_temp--) if(grid[# xx_temp, 2] == WALL) wall_close = true;
        
        if(!wall_close)
        {
            for(yy = 2; yy &lt; height / 2 - 3; yy++)
            {
                grid[# xx, yy] = WALL;
            }
        } else wall_close = false;
    }
    
    var wall_close = false;
    if(irandom(odds) == odds)
    {
        for(var xx_temp = xx; xx_temp &gt; xx - 8 &amp;&amp; !wall_close; xx_temp--) if(grid[# xx_temp, height / 2] == WALL) wall_close = true;
        
        if(!wall_close)
        {
            for(yy = height - 2; yy &gt; (height / 2) - 1; yy--)
            {
                grid[# xx, yy] = WALL;
            }
        } else wall_close = false;
    }
}

// Sets controller
var cx = width - 10;
var cy = height - 10;

// Makes sure player doesnt spawn inside a wall os object
while(grid[# cx, cy] == WALL or grid[# cx, cy] == OBSTACLE)
{
    cx--;
    cy--
}

// Place player in the room
instance_create(cx * CELL_WIDTH + CELL_WIDTH / 2, cy * CELL_HEIGHT + CELL_HEIGHT / 2, obj_player);


// Draw the level using the grid
for(var yy = 1; yy &lt; height; yy++)
{
    for(var xx = 1; xx &lt; width; xx++)
    {
        if(grid[# xx, yy] == FLOOR || grid[# xx, yy] == OBSTACLE) 
        {
            tile_add(bg_tile_floor, 0, 0, CELL_WIDTH, CELL_HEIGHT, xx * CELL_WIDTH, yy * CELL_HEIGHT, 0);
            
            if(grid[# xx, yy] == FLOOR)
            {
                odds = 20;
                var ex = xx * CELL_WIDTH + CELL_WIDTH / 2;
                var ey = yy * CELL_HEIGHT + CELL_HEIGHT / 2;
                if(point_distance(ex, ey, obj_player.x, obj_player.y) &gt; 151 &amp;&amp;
                    irandom(odds) == odds)
                {
                    instance_create(ex, ey, obj_enemy);
                }
            }
            
            if(grid[# xx, yy] == OBSTACLE) 
            {
                var ex = xx * CELL_WIDTH + CELL_WIDTH / 2;
                var ey = yy * CELL_HEIGHT + CELL_HEIGHT / 2;
                
                if(irandom(1) == 1)
                    instance_create(ex, ey, obj_bottle);
                else
                    instance_create(ex, ey, obj_trash);
                    
               
            }
        }
        
        if(grid[# xx, yy] != FLOOR)  mp_grid_add_cell(grid_path, xx, yy);
    }
}


// Get tile sizes
var tw = CELL_WIDTH / 2;
var th = CELL_HEIGHT / 2;

// Add the tiles
for (var yy = 0; yy &lt; height * 2; yy++)
{
    for(var xx = 0; xx &lt; width * 2; xx++)
    {
        if(grid[# xx div 2, yy div 2] == FLOOR or grid[# xx div 2, yy div 2] == OBSTACLE)
        {
            // Get the tile's position
            var tx = xx * tw;
            var ty = yy * th;
            
            var right = grid[# (xx + 1) div 2, yy div 2] != FLOOR &amp;&amp; grid[# (xx + 1) div 2, yy div 2] != OBSTACLE;
            var left = grid[# (xx - 1) div 2, yy div 2] != FLOOR &amp;&amp; grid[# (xx - 1) div 2, yy div 2] != OBSTACLE;
            var top = grid[# xx div 2, (yy - 1) div 2] != FLOOR &amp;&amp; grid[# xx div 2, (yy - 1) div 2] != OBSTACLE;
            var bottom = grid[# xx div 2, (yy + 1) div 2] != FLOOR &amp;&amp; grid[# xx div 2, (yy + 1) div 2] != OBSTACLE;
            
            var top_right = grid[# (xx + 1) div 2, (yy - 1) div 2] != FLOOR &amp;&amp; grid[# (xx + 1) div 2, (yy - 1) div 2] != OBSTACLE;
            var top_left = grid[# (xx - 1) div 2, (yy - 1) div 2] != FLOOR &amp;&amp; grid[# (xx - 1) div 2, (yy - 1) div 2] != OBSTACLE;
            var bottom_right = grid[# (xx + 1) div 2, (yy + 1) div 2] != FLOOR &amp;&amp; grid[# (xx + 1) div 2, (yy + 1) div 2] != OBSTACLE;
            var bottom_left = grid[# (xx - 1) div 2, (yy + 1) div 2] != FLOOR &amp;&amp; grid[# (xx - 1) div 2, (yy + 1) div 2] != OBSTACLE;
            
            if(right)
            {
                if(bottom)
                {
                    tile_add(bg_tiles, tw * 4, th * 1, tw, th, tx + tw, ty, -ty - 1);
                } else if(top)
                {
                    if (top_right)
                    {
                        tile_add(bg_tiles, tw * 4, th * 0, tw, th, tx + tw, ty - th, -ty - 1);
                    } else
                    {
                        tile_add(bg_tiles, tw * 3, th * 0, tw, th, tx, ty - th, -ty - 1);
                    }
                    tile_add(bg_tiles, tw * 0, th * 1, tw, th, tx + tw, ty, -ty - 1);
                } else tile_add(bg_tiles, tw * 0, th * 1, tw, th, tx + tw, ty, -ty - 1);
            }
            
            if(left)
            {
                if (bottom)
                {
                    tile_add(bg_tiles, tw * 3, th * 1, tw, th, tx - tw, ty, -ty - 1);
                } else if (top)
                {
                    if(top_left)
                    {
                        tile_add(bg_tiles, tw * 3, th * 0, tw, th, tx - tw, ty - th, -ty - 1);
                    } else
                    {
                        tile_add(bg_tiles, tw * 4, th * 0, tw, th, tx, ty - th, -ty - 1);
                    }
                    tile_add(bg_tiles, tw * 2, th * 1, tw, th, tx - tw, ty, -ty - 1);
                } else
                {
                    tile_add(bg_tiles, tw * 2, th * 1, tw, th, tx - tw, ty, -ty - 1);
                }
            }
            
            if (top)
            {
                if (!top_right)
                {
                    tile_add(bg_tiles, tw * 2, th * 2, tw, th, tx, ty - th, -ty - 1);
                } else if(!top_left)
                {
                    tile_add(bg_tiles, tw * 0, th * 2, tw, th, tx, ty - th, -ty - 1);
                } else {
                    tile_add(bg_tiles, tw * 1, th * 2, tw, th, tx, ty - th, -ty - 1);
                }
            }
            
            if (bottom)
            {
                if (!bottom_right)
                {
                    tile_add(bg_tiles, tw * 2, th * 0, tw, th, tx, ty, -ty - 1);
                } else if(!bottom_left)
                {
                    tile_add(bg_tiles, tw * 0, th * 0, tw, th, tx, ty, -ty - 1);
                } else {
                    tile_add(bg_tiles, tw * 1, th * 0, tw, th, tx, ty, -ty - 1);
                }
            }
        }
    }
}

</string>
          </argument>
        </arguments>
      </action>
